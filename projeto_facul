DROP TABLE IF EXISTS `banco`.`telefone_orgao` ;

CREATE TABLE IF NOT EXISTS `banco`.`telefone_orgao` (
  `idt_telefone` INT NOT NULL AUTO_INCREMENT,
  `telefone` VARCHAR(45) NOT NULL,
  `orgao_CNPJ` VARCHAR(40) NOT NULL,
  PRIMARY KEY (`idt_telefone`),
  INDEX `fk_telefone_orgao_orgao1_idx` (`orgao_CNPJ` ASC) VISIBLE,
  CONSTRAINT `fk_telefone_orgao_orgao1`
    FOREIGN KEY (`orgao_CNPJ`)
    REFERENCES `banco`.`orgao` (`CNPJ`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


 INSERT INTO telefone_orgao (telefone, orgao_CNPJ)
SELECT CONCAT('telefone', FLOOR(RAND()*1000)), CNPJ
FROM orgao
ORDER BY RAND()
LIMIT 1000;


 SELECT * FROM telefone_orgao;

DROP TABLE IF EXISTS `banco`.`situação_vagas_cargos` ;

CREATE TABLE IF NOT EXISTS `banco`.`situação_vagas_cargos` (
  `idt_situacao` INT NOT NULL AUTO_INCREMENT,
  `situacao` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`idt_situacao`))
ENGINE = InnoDB;
 
 INSERT INTO situação_vagas_cargos(situacao) VALUES
("desocupadas"),
("ocupadas"),
("disputadas");

SELECT *FROM situação_vagas_cargos;

DROP TABLE IF EXISTS `banco`.`cargos` ;

CREATE TABLE IF NOT EXISTS `banco`.`cargos` (
  `código` INT NOT NULL AUTO_INCREMENT,
  `nome` VARCHAR(45) NOT NULL,
  `salário` DECIMAL(10,2) NOT NULL,
  `situação_vagas_cargos_idt_situacao` INT NOT NULL,
  PRIMARY KEY (`código`),
  INDEX `fk_cargos_situação_vagas_cargos1_idx` (`situação_vagas_cargos_idt_situacao` ASC) VISIBLE,
  CONSTRAINT `fk_cargos_situação_vagas_cargos1`
    FOREIGN KEY (`situação_vagas_cargos_idt_situacao`)
    REFERENCES `banco`.`situação_vagas_cargos` (`idt_situacao`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

