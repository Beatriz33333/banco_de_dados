DROP SCHEMA IF EXISTS `banco` ;

CREATE SCHEMA IF NOT EXISTS `banco` DEFAULT CHARACTER SET utf8 ;

USE `banco` ;

DROP TABLE IF EXISTS `banco`.`ME` ;

CREATE TABLE IF NOT EXISTS `banco`.`ME` (
  `CNPJ` VARCHAR(30) NOT NULL,
  `nome` VARCHAR(30) NOT NULL,
  `sigla` VARCHAR(2) NOT NULL,
  PRIMARY KEY (`CNPJ`))
ENGINE = InnoDB;

INSERT INTO `banco`.`ME` (`CNPJ`, `nome`, `sigla`) VALUES
 ('00.394.460/0001-41', 'Ministério da Economia', 'ME');
 
 SELECT * FROM ME;

DROP TABLE IF EXISTS `banco`.`telefone_ME` ;

CREATE TABLE IF NOT EXISTS `banco`.`telefone_ME` (
  `idt_Telefone_ME` INT NOT NULL AUTO_INCREMENT,
  `telefone` VARCHAR(45) NOT NULL,
  `FK_ME_CNPJ` VARCHAR(30) NOT NULL,
  PRIMARY KEY (`idt_Telefone_ME`),
  INDEX `fk_telefone_ME_ME_idx` (`FK_ME_CNPJ` ASC) VISIBLE,
  CONSTRAINT `fk_telefone_ME_ME`
    FOREIGN KEY (`FK_ME_CNPJ`)
    REFERENCES `banco`.`ME` (`CNPJ`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

INSERT INTO telefone_ME(FK_ME_CNPJ, telefone) VALUES
