DROP TABLE IF EXISTS `banco`.`pessoa` ;

CREATE TABLE IF NOT EXISTS `banco`.`pessoa` (
  `CPF` VARCHAR(20) NOT NULL,
  `nome` VARCHAR(20) NOT NULL,
  `data_nasc` VARCHAR(20) NOT NULL,
PRIMARY KEY (`CPF`))
ENGINE = InnoDB;

INSERT INTO pessoa(CPF, nome, data_nasc)
SELECT CONCAT(FLOOR(RAND() * 10000000000), FLOOR(RAND() * 100000)),
  CONCAT('Nome', FLOOR(RAND() * 1000)),
  DATE_SUB(NOW(), INTERVAL FLOOR(RAND() * 10000) DAY)
FROM information_schema.tables
LIMIT 1000;

SELECT * FROM pessoa;

DROP TABLE IF EXISTS `banco`.`endereco_pessoa` ;

CREATE TABLE IF NOT EXISTS `banco`.`endereco_pessoa` (
  `idt_endereco` INT NOT NULL AUTO_INCREMENT,
  `endereco` VARCHAR(100) NOT NULL,
  `pessoa_CPF` VARCHAR(20) NOT NULL,
  PRIMARY KEY (`idt_endereco`),
  INDEX `fk_endereco_pessoa_pessoa1_idx` (`pessoa_CPF` ASC) VISIBLE,
  CONSTRAINT `fk_endereco_pessoa_pessoa1`
    FOREIGN KEY (`pessoa_CPF`)
    REFERENCES `banco`.`pessoa` (`CPF`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

INSERT INTO endereco_pessoa (endereco, pessoa_CPF)
SELECT CONCAT('Endereço ', n), CPF
FROM pessoa
CROSS JOIN (
    SELECT @row_number:=@row_number+1 AS n
    FROM information_schema.tables, (SELECT @row_number:=0) AS t
    LIMIT 1000
) AS numbers;

SELECT * FROM endereco_pessoa;

DROP TABLE IF EXISTS `banco`.`telefone_pessoa` ;

CREATE TABLE IF NOT EXISTS `banco`.`telefone_pessoa` (
  `idt_telefone_pessoa` INT NOT NULL AUTO_INCREMENT,
  `telefone_pessoa` VARCHAR(20) NOT NULL,
  `pessoa_CPF` VARCHAR(20) NOT NULL,
  PRIMARY KEY (`idt_telefone_pessoa`),
  INDEX `fk_telefone_pessoa_pessoa1_idx` (`pessoa_CPF` ASC) VISIBLE,
  CONSTRAINT `fk_telefone_pessoa_pessoa1`
    FOREIGN KEY (`pessoa_CPF`)
    REFERENCES `banco`.`pessoa` (`CPF`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

ALTER TABLE telefone_pessoa MODIFY COLUMN telefone_pessoa VARCHAR(50);

INSERT INTO telefone_pessoa (telefone_pessoa, pessoa_CPF)
 SELECT CONCAT('(', FLOOR(RAND() * 1000), ') 555-', FLOOR(RAND() * 10000), ', ramal ', FLOOR(RAND() * 100))
 AS telefone, CPF
 FROM pessoa
 CROSS JOIN (
     SELECT @row_number:=@row_number+1 AS n
     FROM information_schema.tables, (SELECT @row_number:=0) AS t
     LIMIT 1000
 ) AS numbers;



SELECT * FROM telefone_pessoa;


DROP TABLE IF EXISTS `banco`.`email_pessoa` ;

CREATE TABLE IF NOT EXISTS `banco`.`email_pessoa` (
  `idt_email_pessoa` INT NOT NULL AUTO_INCREMENT,
  `email` VARCHAR(45) NOT NULL,
  `pessoa_CPF` VARCHAR(20) NOT NULL,
  PRIMARY KEY (`idt_email_pessoa`),
  INDEX `fk_email_pessoa_pessoa1_idx` (`pessoa_CPF` ASC) VISIBLE,
  CONSTRAINT `fk_email_pessoa_pessoa1`
    FOREIGN KEY (`pessoa_CPF`)
    REFERENCES `banco`.`pessoa` (`CPF`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

INSERT INTO email_pessoa(pessoa_CPF, email) 
SELECT CPF, CONCAT(nome, '@exemplo.com')
FROM pessoa;


SELECT *FROM email_pessoa;

DROP TABLE IF EXISTS `banco`.`aprovados`;

CREATE TABLE IF NOT EXISTS `banco`.`aprovados` (
  `idt_aprovados` INT NOT NULL AUTO_INCREMENT,
  `adicionais` DECIMAL(10,2) NOT NULL,
  `nota` DECIMAL(4,2) NOT NULL,
  `classificacao` INT(10) NOT NULL,
  `pessoa_CPF` VARCHAR(20) NOT NULL,
  PRIMARY KEY (`idt_aprovados`),
  INDEX `fk_aprovados_pessoa1_idx` (`pessoa_CPF` ASC) VISIBLE,
  CONSTRAINT `fk_aprovados_pessoa1`
    FOREIGN KEY (`pessoa_CPF`)
    REFERENCES `banco`.`pessoa` (`CPF`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION
)
ENGINE = InnoDB;

INSERT INTO `banco`.`aprovados` (`adicionais`, `nota`, `classificacao`, `pessoa_CPF`)
SELECT ROUND(RAND() * 100, 2), ROUND(RAND() * 4, 2), FLOOR(RAND() * 1000), `CPF`
FROM `banco`.`pessoa`
WHERE `CPF` IS NOT NULL
LIMIT 1000;



SELECT * FROM `banco`.`aprovados`;


DROP TABLE IF EXISTS `banco`.`gestao` ;

CREATE TABLE IF NOT EXISTS `banco`.`gestao` (
  `data` DATE NOT NULL,
  `orgao_CNPJ` VARCHAR(30) NOT NULL,
  `cargos_código` INT NOT NULL,
  INDEX `fk_gestao_orgao1_idx` (`orgao_CNPJ` ASC) VISIBLE,
  INDEX `fk_gestao_cargos1_idx` (`cargos_código` ASC) VISIBLE,
  PRIMARY KEY (`cargos_código`, `orgao_CNPJ`),
  CONSTRAINT `fk_gestao_orgao1`
    FOREIGN KEY (`orgao_CNPJ`)
    REFERENCES `banco`.`orgao` (`CNPJ`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_gestao_cargos1`
    FOREIGN KEY (`cargos_código`)
    REFERENCES `banco`.`cargos` (`código`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

INSERT INTO `banco`.`gestao` (`data`, `orgao_CNPJ`, `cargos_código`)
SELECT CURDATE(), `CNPJ`, `código`
FROM `banco`.`orgao`, `banco`.`cargos`
WHERE `orgao`.`CNPJ` IS NOT NULL AND `cargos`.`código` IS NOT NULL
LIMIT 1000;

SELECT * FROM `banco`.`gestao`;


DROP TABLE IF EXISTS `banco`.`nomeia`;

CREATE TABLE IF NOT EXISTS `banco`.`nomeia` (
  `data_nomeacao` INT NOT NULL,
  `aprovados_idt_aprovados` INT NOT NULL,
  `cargos_código` INT NOT NULL,
  INDEX `fk_nomeia_aprovados2_idx` (`aprovados_idt_aprovados` ASC) VISIBLE,
  INDEX `fk_nomeia_cargos1_idx` (`cargos_código` ASC) VISIBLE,
  PRIMARY KEY (`aprovados_idt_aprovados`, `cargos_código`),
  CONSTRAINT `fk_nomeia_aprovados2`
    FOREIGN KEY (`aprovados_idt_aprovados`)
    REFERENCES `banco`.`aprovados` (`idt_aprovados`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_nomeia_cargos1`
    FOREIGN KEY (`cargos_código`)
    REFERENCES `banco`.`cargos` (`código`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION
)
ENGINE = InnoDB;

ALTER TABLE `banco`.`nomeia`
MODIFY COLUMN `data_nomeacao` DATE NOT NULL;

INSERT INTO `banco`.`nomeia` (`data_nomeacao`, `aprovados_idt_aprovados`, `cargos_código`)
SELECT DATE_ADD('2000-01-01', INTERVAL FLOOR(RAND() * 8495) DAY), `aprovados`.`idt_aprovados`, `cargos`.`código`
FROM `banco`.`aprovados`
JOIN `banco`.`cargos` ON 1
LIMIT 1000;


SELECT COUNT(*) FROM `banco`.`nomeia`;

SELECT * FROM `banco`.`nomeia`;


DROP TABLE IF EXISTS `banco`.`passar`;

CREATE TABLE IF NOT EXISTS `banco`.`passar` (
  `nomeia_aprovados_idt_aprovados` INT NOT NULL,
  `nomeia_cargos_código` INT NOT NULL,
  `aprovados_idt_aprovados` INT NOT NULL,
  INDEX `fk_passar_nomeia1_idx` (`nomeia_aprovados_idt_aprovados` ASC, `nomeia_cargos_código` ASC) VISIBLE,
  INDEX `fk_passar_aprovados1_idx` (`aprovados_idt_aprovados` ASC) VISIBLE,
  PRIMARY KEY (`nomeia_cargos_código`, `aprovados_idt_aprovados`, `nomeia_aprovados_idt_aprovados`),
  CONSTRAINT `fk_passar_nomeia1`
    FOREIGN KEY (`nomeia_aprovados_idt_aprovados`, `nomeia_cargos_código`)
    REFERENCES `banco`.`nomeia` (`aprovados_idt_aprovados`, `cargos_código`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_passar_aprovados1`
    FOREIGN KEY (`aprovados_idt_aprovados`)
    REFERENCES `banco`.`aprovados` (`idt_aprovados`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION
)
ENGINE = InnoDB;

INSERT INTO `banco`.`passar` (`nomeia_aprovados_idt_aprovados`, `nomeia_cargos_código`, `aprovados_idt_aprovados`)
SELECT `nomeia`.`aprovados_idt_aprovados`, `nomeia`.`cargos_código`, `aprovados`.`idt_aprovados`
FROM `banco`.`nomeia`
JOIN `banco`.`aprovados` ON `nomeia`.`aprovados_idt_aprovados` = `aprovados`.`idt_aprovados`
LIMIT 1000;

SELECT * FROM `banco`.`passar`;


DROP TABLE IF EXISTS `banco`.`posse` ;

CREATE TABLE IF NOT EXISTS `banco`.`posse` (
  `cargos_código` INT NOT NULL,
  `nomeia_aprovados_idt_aprovados` INT NOT NULL,
  `nomeia_cargos_código` INT NOT NULL,
  INDEX `fk_table1_cargos1_idx` (`cargos_código` ASC) VISIBLE,
  INDEX `fk_table1_nomeia1_idx` (`nomeia_aprovados_idt_aprovados` ASC, `nomeia_cargos_código` ASC) VISIBLE,
  PRIMARY KEY (`cargos_código`, `nomeia_aprovados_idt_aprovados`, `nomeia_cargos_código`),
  CONSTRAINT `fk_table1_cargos1`
    FOREIGN KEY (`cargos_código`)
    REFERENCES `banco`.`cargos` (`código`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_table1_nomeia1`
    FOREIGN KEY (`nomeia_aprovados_idt_aprovados` , `nomeia_cargos_código`)
    REFERENCES `banco`.`nomeia` (`aprovados_idt_aprovados` , `cargos_código`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

INSERT INTO `banco`.`posse` (`cargos_código`, `nomeia_aprovados_idt_aprovados`, `nomeia_cargos_código`)
SELECT `nomeia`.`cargos_código`, `nomeia`.`aprovados_idt_aprovados`, `nomeia`.`cargos_código`
FROM `banco`.`nomeia`
LIMIT 1000;

SELECT * FROM banco.posse;


DROP TABLE IF EXISTS `banco`.`entrevista`;

CREATE TABLE IF NOT EXISTS `banco`.`entrevista` (
   `data` DATE NOT NULL,
   `local` VARCHAR(100) NOT NULL,
   `horario` DATETIME NOT NULL,
   `nomeia_aprovados_idt_aprovados` INT NOT NULL,
   `nomeia_cargos_codigo` INT NOT NULL,
   PRIMARY KEY (`nomeia_aprovados_idt_aprovados`, `nomeia_cargos_codigo`),
   INDEX `primarykey` (`data`, `local`, `horario`) INVISIBLE,
   INDEX `fk_entrevista_nomeia1_idx` (`nomeia_aprovados_idt_aprovados`, `nomeia_cargos_codigo`) VISIBLE,
   CONSTRAINT `fk_entrevista_nomeia1`
     FOREIGN KEY (`nomeia_aprovados_idt_aprovados`, `nomeia_cargos_codigo`)
     REFERENCES `banco`.`nomeia` (`aprovados_idt_aprovados`, `cargos_código`)
     ON DELETE NO ACTION
     ON UPDATE NO ACTION
 ) ENGINE=InnoDB;

SELECT aprovados_idt_aprovados, cargos_código 
FROM nomeia;

INSERT INTO `banco`.`entrevista` (`data`, `local`, `horario`, `nomeia_aprovados_idt_aprovados`, `nomeia_cargos_codigo`)
SELECT
    DATE(DATE_ADD(NOW(), INTERVAL FLOOR(RAND() * 365) DAY)), 
    CONCAT('Local ', FLOOR(RAND() * 1000)), 
    DATE_ADD(NOW(), INTERVAL FLOOR(RAND() * 24 * 60 * 60) SECOND), 
    n.aprovados_idt_aprovados, 
    n.cargos_código
FROM
    (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5) AS t1
    CROSS JOIN
    (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5) AS t2
    JOIN `banco`.`nomeia` AS n ON n.data_nomeacao = (
        SELECT data_nomeacao
        FROM `banco`.`nomeia`
        ORDER BY RAND()
        LIMIT 1
    )
LIMIT 1000;

SELECT * FROM entrevista;
USE `banco`
