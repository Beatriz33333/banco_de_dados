DROP TABLE IF EXISTS `banco`.`orgao` ;

CREATE TABLE IF NOT EXISTS `banco`.`orgao` (
  `CNPJ` VARCHAR(30) NOT NULL,
  `nome` VARCHAR(100) NOT NULL,
  `sigla` VARCHAR(10) NOT NULL,
  `filho` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`CNPJ`))
ENGINE = InnoDB;

